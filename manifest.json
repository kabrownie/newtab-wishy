{
"manifest_version": 2,
"name" : "wishy",
"description": "feel motivated every time you open a new tab :)",
"version" : "1.0",
"author":"Kevin Karanja",
 
    "browser_action":{"default_icon": "tab-icon.png",
    "default_title":"Hello there :)",
    "default_popup":"popup.html"
    },
    "chrome_url_overrides" :
     {
        "newtab":"newtab.html"
    },
    "permissions":["activeTab"]
}